{
  "root": "apps/ast-type-gen",
  "sourceRoot": "apps/ast-type-gen/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/ast-type-gen",
        "main": "apps/ast-type-gen/src/main.ts",
        "tsConfig": "apps/ast-type-gen/tsconfig.app.json"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/ast-type-gen/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/ast-type-gen"],
      "options": {
        "jestConfig": "apps/ast-type-gen/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
